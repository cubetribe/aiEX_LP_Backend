{
  "name": "goaix-backend",
  "version": "0.1.0",
  "description": "AI-Lead-Magnet-Plattform - Backend API mit Strapi v4",
  "keywords": [
    "strapi",
    "ai",
    "lead-generation",
    "landing-pages",
    "campaigns",
    "headless-cms"
  ],
  "author": "GoAIX Development Team",
  "license": "MIT",
  "main": "./src/index.js",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "develop": "strapi develop",
    "start": "strapi start",
    "build": "strapi build",
    "strapi": "strapi",
    "strapi:build": "strapi build --clean",
    "strapi:develop": "strapi develop --watch-admin",
    "strapi:start": "NODE_ENV=production strapi start",
    "strapi:export": "strapi export",
    "strapi:import": "strapi import",
    "strapi:migrate": "strapi db:migrate",
    "strapi:seed": "strapi db:seed",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=tests/unit",
    "test:integration": "jest --testPathPattern=tests/integration",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write '**/*.{js,jsx,ts,tsx,json,md}'",
    "format:check": "prettier --check '**/*.{js,jsx,ts,tsx,json,md}'",
    "typecheck": "tsc --noEmit",
    "queue:start": "node scripts/start-queue-worker.js",
    "queue:dashboard": "node scripts/start-queue-dashboard.js",
    "db:reset": "strapi db:drop && strapi db:migrate",
    "db:seed": "node scripts/seed-database.js",
    "deploy:staging": "npm run build && npm run strapi:migrate && npm run strapi:start",
    "deploy:production": "npm run build && npm run strapi:migrate && pm2 start ecosystem.config.js --env production",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:restart": "pm2 restart ecosystem.config.js",
    "pm2:logs": "pm2 logs",
    "backup:db": "node scripts/backup-database.js",
    "health:check": "node scripts/health-check.js",
    "analyze:bundle": "npm run build && npx webpack-bundle-analyzer build/static/js/*.js"
  },
  "dependencies": {
    "@strapi/strapi": "^4.24.2",
    "@strapi/plugin-users-permissions": "^4.24.2",
    "@strapi/plugin-i18n": "^4.24.2",
    "@strapi/plugin-documentation": "^4.24.2",
    "@strapi/plugin-graphql": "^4.24.2",
    "@strapi/provider-upload-local": "^4.24.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.2.1",
    "pg": "^8.11.3",
    "redis": "^4.6.10",
    "bull": "^4.12.2",
    "@bull-board/api": "^5.10.2",
    "@bull-board/koa": "^5.10.2",
    "ioredis": "^5.3.2",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "joi": "^17.11.0",
    "axios": "^1.6.2",
    "openai": "^4.20.1",
    "@anthropic-ai/sdk": "^0.24.3",
    "@google/generative-ai": "^0.1.3",
    "googleapis": "^128.0.0",
    "google-auth-library": "^9.4.0",
    "nodemailer": "^6.9.7",
    "handlebars": "^4.7.8",
    "moment": "^2.29.4",
    "lodash": "^4.17.21",
    "uuid": "^9.0.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "express-rate-limit": "^7.1.5",
    "compression": "^1.7.4",
    "dotenv": "^16.3.1",
    "cron": "^3.1.6",
    "sharp": "^0.32.6",
    "multer": "^1.4.5-lts.1",
    "csv-parser": "^3.0.0",
    "csv-writer": "^1.6.0",
    "slugify": "^1.6.6",
    "validator": "^13.11.0",
    "node-cache": "^5.1.2",
    "pm2": "^5.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.9.0",
    "@types/jest": "^29.5.8",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/lodash": "^4.14.202",
    "@types/uuid": "^9.0.7",
    "@types/validator": "^13.11.6",
    "@types/multer": "^1.4.11",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "typescript": "^5.2.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.54.0",
    "@eslint/js": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-security": "^1.7.1",
    "prettier": "^3.1.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "cross-env": "^7.0.3",
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.2",
    "webpack-bundle-analyzer": "^4.10.1"
  },
  "strapi": {
    "uuid": "generate-new-uuid-for-project"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/tests/**/*.test.js",
      "**/tests/**/*.spec.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*.test.js",
      "!src/**/*.spec.js",
      "!src/index.js"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setup.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-org/goaix-backend.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/goaix-backend/issues"
  },
  "homepage": "https://github.com/your-org/goaix-backend#readme"
}