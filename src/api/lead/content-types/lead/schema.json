{
  "kind": "collectionType",
  "collectionName": "leads",
  "info": {
    "singularName": "lead",
    "pluralName": "leads",
    "displayName": "Lead",
    "description": "Lead submissions from campaigns"
  },
  "options": {
    "draftAndPublish": false,
    "comment": ""
  },
  "pluginOptions": {
    "content-manager": {
      "visible": true
    },
    "content-type-builder": {
      "visible": true
    }
  },
  "attributes": {
    "firstName": {
      "type": "string",
      "required": true,
      "maxLength": 100
    },
    "email": {
      "type": "email",
      "required": true
    },
    "responses": {
      "type": "json",
      "required": true,
      "default": {}
    },
    "campaign": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::campaign.campaign",
      "inversedBy": "leads"
    },
    "leadScore": {
      "type": "integer",
      "min": 0,
      "max": 100,
      "default": 0
    },
    "leadQuality": {
      "type": "enumeration",
      "enum": ["hot", "warm", "cold", "unqualified"],
      "default": "cold"
    }
  }
}